server <- function(input, output, session) {
output$plot <- renderPlot({
spacemission_dates %>%
ggplot(aes_string(x=year, y=input$x, fill = input$x))+
geom_col()
}) #closes render plot
session$onSessionEnded(stopApp)
} #closes session
shinyApp(ui, server)
